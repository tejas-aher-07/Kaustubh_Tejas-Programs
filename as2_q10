create procedure Emp_Details()

BEGIN
	declare _deptno1,_deptno2,_deptno3,_deptno4,_deptno5 int;
	declare _ename varchar(20);
	declare c1 cursor for select * from Emp_detail where deptno=10;
	declare c2 cursor for select * from Emp_detail where deptno=20;
	declare c3 cursor for select * from Emp_detail where deptno=30;
	declare c4 cursor for select * from Emp_detail where deptno=40;
	declare c5 cursor for select * from Emp_detail where deptno=50;
	
	open c1;
	open c2;
	open c3;
	open c4;
	open c5;
	
	loop11:loop
		
		fetch c1 into _deptno1, _ename;
		fetch c2 into _deptno2, _ename;
		fetch c3 into _deptno3, _ename;
		fetch c4 into _deptno4, _ename;
		fetch c5 into _deptno5, _ename;
		
		if(_deptno1 = 10)
			select _deptno as 'Department Number ',"->",_ename as 'Employee Name';
		
		elseif(_deptno2 = 20)
			select _deptno as 'Department Number ',"->",_ename as 'Employee Name';
			
		elseif(_deptno3 = 30)
			select _deptno as 'Department Number ',"->",_ename as 'Employee Name';
			
		elseif(_deptno4 = 40)
			select _deptno as 'Department Number ',"->",_ename as 'Employee Name';
			
		elseif(_deptno5 = 50)
			select _deptno as 'Department Number ',"->",_ename as 'Employee Name';
			
		else 
			leave loop11;
		
		end if;

	end loop loop11;
	close c1;
	close c2;
	close c3;
	close c4;
	close c5;
end $ 

delimiter ;
